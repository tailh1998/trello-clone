{
  "name": "trello",
  "private": true,
  "version": "2.0.0",
  "license": "MIT",
  "description": "https://github.com/tailh1998/trello-api-v2",
  "author": "tailh1998 <taile06121998@gmail.com>",
  "engines": {
    "node": "20.8.0",
    "npm": "please-use-yarn"
  },
  "main": "./src/index.ts",
  "scripts": {
    "clean": "rimraf dist && rimraf dist-webpack && rimraf dist-babel && rimraf dist-tsc && rimraf reports && rimraf build && rimraf tsconfig.tsbuildinfo",
    "build:babel": "cross-env NODE_ENV=production babel --config-file ./babel.config.js ./src --extensions .ts --out-dir ./dist/babel",
    "build:webpack": "cross-env NODE_ENV=production webpack --config=webpack.config.js --mode=production --color",
    "build:tsc": "tsc --build",
    "build": "npm run clean && run-p -l build:*",
    "start": "nodemon --inspect ./src/index.ts",
    "dev": "nodemon --inspect ./src/index.ts",
    "cspell:check": "cspell \"**\"",
    "eslint:check": "eslint src --color --ext js --report-unused-disable-directives --max-warnings 0",
    "eslint:fix": "yarn eslint:check --fix",
    "prettier:check": "prettier --check --ignore-unknown src",
    "prettier:fix": "prettier --write --ignore-unknown src",
    "prepare": "husky install",
    "type-check": "tsc --traceResolution"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.9",
    "@babel/runtime-corejs3": "^7.23.9",
    "bcrypt": "^5.1.1",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.2",
    "es6-promise": "^4.2.8",
    "express": "^4.18.2",
    "http-status-codes": "^2.3.0",
    "joi": "^17.12.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^8.1.1",
    "slugify": "^1.6.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.10",
    "@babel/eslint-parser": "^7.22.10",
    "@babel/node": "^7.22.10",
    "@babel/plugin-transform-modules-commonjs": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/register": "^7.23.7",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/webpack-node-externals": "^3.0.4",
    "app-root-path": "^3.1.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "compression": "^1.7.4",
    "cspell": "^8.1.3",
    "dotenv-expand": "^11.0.2",
    "eslint": "^8.55.0",
    "file-loader": "^6.2.0",
    "helmet": "^7.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.2",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
