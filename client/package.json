{
  "name": "trello",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "author": "Lê Hữu Tài <taile06121998@gmail.com>",
  "description": "Lê Hữu Tài - Modern Trello Clone Application in NuxtJS with TypeScript | Vuetify | Pinia | Vue Draggable | VueQuery",
  "engines": {
    "node": "20.8.0",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "build": "nuxt build --dotenv env/.env.production",
    "dev": "nuxt dev --host --dotenv env/.env.development",
    "start": "nuxt dev --host --dotenv env/.env.development",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "npx only-allow yarn && nuxt prepare",
    "cspell:check": "cspell \"**\"",
    "eslint:check": "eslint \"**/*.{ts,tsx,js,jsx,vue}\" --color",
    "eslint:fix": "yarn eslint:check --fix",
    "prettier:check": "prettier --check --ignore-unknown .",
    "prettier:fix": "prettier --write --ignore-unknown .",
    "type:check": "yarn nuxi typecheck",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@brucesong/eslint-config-vue": "^1.0.19",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@nuxt/devtools": "latest",
    "@nuxt/image": "^1.1.0",
    "@nuxtjs/i18n": "^8.0.0-rc.11",
    "@pinia/nuxt": "^0.5.1",
    "@types/lodash": "^4.14.202",
    "@types/nprogress": "^0.2.3",
    "@vueuse/core": "^10.7.0",
    "@vueuse/nuxt": "^10.7.0",
    "autoprefixer": "^10.4.16",
    "cspell": "^8.1.3",
    "eslint": "^8.55.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "nuxt": "^3.8.2",
    "nuxt-icon": "^0.6.7",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "vite-plugin-vuetify": "^2.0.1",
    "vue": "^3.3.10",
    "vue-router": "^4.2.5",
    "vuetify": "^3.4.7"
  },
  "dependencies": {
    "@hebilicious/vue-query-nuxt": "^0.2.1",
    "@mdi/font": "^7.3.67",
    "@tanstack/vue-query": "^5.14.0",
    "@vee-validate/nuxt": "^4.12.3",
    "@vee-validate/zod": "^4.12.3",
    "@vueuse/motion": "^2.0.0",
    "axios": "^1.6.2",
    "dayjs": "^1.11.10",
    "lodash": "^4.17.21",
    "maska": "^2.1.10",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "sass": "^1.69.5",
    "vuedraggable": "4.1.0",
    "zod": "^3.22.4"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
